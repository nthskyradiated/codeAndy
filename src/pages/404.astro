---
import Layout from "../layouts/Layout.astro";
import notFound from "../images/404-darktheme.svg"
import { Image } from 'astro:assets';
---

<Layout title="404 Not Found">
    <Image id="notFound" src={notFound} alt="404 Not Found" />
</Layout>

<style lang='scss'>
    #notFound {
        width: 100%;
        max-height: 100vh;
        object-fit: cover;
    }

</style>
